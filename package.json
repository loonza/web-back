    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.14",
